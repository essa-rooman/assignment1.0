  grid-gap: 30px;
  max-width: 900px;
        position: absolute;
        top: 50%;
        left: 30%;
        transform: translate(-50%, -50%);
        text-align: center;
        
}
.header { grid-area: h; }
.art.first { grid-area: a1;
  background-color: lightgrey;
